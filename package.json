        "eslint": "^8.57.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^17.6.0",
        "eslint-plugin-promise": "^6.0.0",
        "typescript": "latest",
        "@vercel/ncc": "^0.38.1"
    },
    "dependencies": {
        "@actions/core": "^1.10.1",
        "axios": "^1.6.8",
        "@azure/arm-apimanagement": "^9.1.0",
        "@azure/identity": "^4.2.0",
